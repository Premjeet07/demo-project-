<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.infinite.jsf.admin.model.Doctor" table="Doctors">
        <id name="doctorId" column="doctor_id" type="string">
            <generator class="assigned"/>
        </id>

        <many-to-one name="provider" class="com.infinite.jsf.admin.model.Provider" column="provider_id" not-null="false"/>

        <property name="doctorName" column="doctor_name" type="string"/>
        <property name="qualification" column="qualification" type="string"/>
        <property name="specialization" column="specialization" type="string"/>
         <property name="licenseNo" column="license_no" type="string"/>
        <property name="email" column="email" type="string"/>
        <property name="address" column="address" type="string"/>
        <property name="gender" column="gender" type="string"/>
        <property name="password" column="password" type="string"/>
        <property name="loginStatus" column="login_status" type="string"/>
        <property name="doctorStatus" column="doctor_status" type="string"/>
    </class>
</hibernate-mapping>
