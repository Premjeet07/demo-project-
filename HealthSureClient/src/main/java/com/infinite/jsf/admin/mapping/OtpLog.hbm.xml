<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.infinite.jsf.admin.model.OtpLog" table="Otp_logs">
        <id name="otpId" column="otp_id" type="int">
            <generator class="increment"/>
        </id>

        <property name="userType" column="user_type" type="string"/>
        <property name="otpCode" column="otp_code" type="string"/>
        <property name="isUsed" column="is_used" type="boolean"/>
        <property name="createdAt" column="created_at" type="timestamp"/>
    </class>
    </hibernate-mapping>