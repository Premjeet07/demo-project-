<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.infinite.jsf.admin.model.InsuranceCompany" table="Insurance_company">
        <id name="companyId" column="company_id">
            <generator class="assigned"/>
        </id>

        <property name="name" column="name" type="string"/>
        <property name="logoUrl" column="logo_url" type="string"/>
        <property name="headOffice" column="head_office" type="string"/>
        <property name="contactEmail" column="contact_email" type="string"/>
        <property name="contactPhone" column="contact_phone" type="string"/>
    </class>
</hibernate-mapping>

