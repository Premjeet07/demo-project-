<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.infinite.jsf.admin.model.ProcedureTest" table="procedure_test">
        <id name="testId" column="test_id" type="string">
            <generator class="assigned"/>
        </id>

        <many-to-one name="procedure" class="com.infinite.jsf.admin.model.MedicalProcedure" column="procedure_id" not-null="true"/>

        <property name="testName" column="test_name" type="string"/>
        <property name="testDate" column="test_date" type="date"/>
        <property name="resultSummary" column="result_summary" type="text"/>
        <property name="status" column="status" type="string"/>
        <property name="createdAt" column="created_at" type="timestamp"/>
    </class>
</hibernate-mapping>
